    "postinstall": "npm run babel",
    "test": "npm run lint && npm run babel && istanbul --config=test/.istanbul.yml cover node_modules/mocha/bin/_mocha -- --check-leaks -b -R spec test/index.js"
  },
  "dependencies": {
    "amqplib": "^0.4.2",
    "babel-runtime": "^6.11.6",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.1",
    "dotenv": "^2.0.0",
    "lodash": "^4.14.1",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "semver": "^5.3.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.12.0",
    "faker": "^3.1.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0",
    "mocking-rabbit": "git+ssh://git@gitlab.com/researcher/mocking-rabbit.git",
