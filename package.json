{
  "name": "chen",
  "version": "0.0.1",
  "author": "gianfebrian<gianfebrian@gmail.com>",
  "description": "Alice, a good friend of rabbits. A higher level API of AMQP",
  "keywords": [
    "alice",
    "rabbit",
    "mq",
    "amqp"
  ],
  "main": "alice.js",
  "scripts": {
    "babel": "babel -D src/ --out-dir lib/ --source-maps inline",
    "dev": "babel -w -D src/ --out-dir lib/ --source-maps inline",
